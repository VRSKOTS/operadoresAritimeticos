#include <stdio.h>
#include <locale.h>
#include<math.h>

main(){

  double num, raiz, quad;
  
  printf("Digite o numero:");
  scanf("%lf", &num);

  if (num >=0){
    raiz = sqrt(num);
    printf("A raiz quadrada e: %lf", raiz);
    
  }else{
    quad = pow(num, 2);
    printf("O quadrado Ã©: %lf", quad);
  }
}
