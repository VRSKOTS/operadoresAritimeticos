#include <stdio.h>

int main() {

    float pesoMaximo = 60.0;

    printf("Digite o peso (em quilogramas): ");
    scanf("%f", &peso);

    if (peso < pesoMaximo) {
        printf("O peso é menor que 60 quilogramas.\n");
    } else {
        printf("O peso é igual ou maior que 60 quilogramas.\n");
    }

    return 0;
}
